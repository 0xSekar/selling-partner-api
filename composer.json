{
    "name": "jlevers/selling-partner-api",
    "version": "5.9.0",
    "description": "PHP client for Amazon's Selling Partner API",
    "keywords": [
        "api",
        "selling-partner-api",
        "amazon",
        "ecommerce",
        "sp-api",
        "sdk",
        "rest",
        "api"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Jesse Evers",
            "homepage": "https://jesseevers.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "phpoffice/phpspreadsheet": "^1.19"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "composer/semver": "^3.4",
        "symfony/console": "^6.3",
        "psy/psysh": "^0.11.22",
        "voku/simple_html_dom": "^4.8"
    },
    "autoload": {
        "psr-4": {
          "SellingPartnerApi\\" : "src/"
        },
        "files": [
          "src/Support/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "SellingPartnerApi\\Tests\\" : "test/" }
    },
    "scripts": {
        "generate": [
            "composer\\config::disableprocesstimeout",
            "php utils/generate-apis --"
        ],
        "refresh-schemas": [
            "@download-schemas",
            "@customize-schemas"
        ],
        "make": [
            "@download-schemas",
            "@customize-schemas",
            "@generate"
        ],
        "version-update": "SellingPartnerApi\\Support\\Commands\\UpdateVersion::execute",
        "test": "vendor/bin/phpunit",
        "clean": "rm -rf docs src/models/*/* src/apis/*/*/*",
        "lint": "php vendor/bin/php-cs-fixer fix --allow-risky=yes -vvv"
    }
}
